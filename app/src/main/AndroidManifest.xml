<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bruceprw.scranshouts">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ScranShouts"
        android:screenOrientation="portrait">

        <activity android:name=".activity_main_feed"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_register"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_login"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_create_review"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_loading"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_welcome"
            android:label="@string/app_name"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity_load_review"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_profile"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_edit_profile"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_user_feed"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_edit_review"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

        <activity android:name=".activity_sorted_feed"
            android:label="@string/app_name"
            android:exported="true">
        </activity>

    </application>



</manifest>